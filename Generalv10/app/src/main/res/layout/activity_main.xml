        app:layout_constraintTop_toBottomOf="@+id/md22" />

    <Button
        android:id="@+id/md12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/md12"
        app:layout_constraintStart_toStartOf="@+id/md11"
        app:layout_constraintTop_toBottomOf="@+id/md11" />

    <Button
        android:id="@+id/md13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/md13"
        app:layout_constraintStart_toStartOf="@+id/md12"
        app:layout_constraintTop_toBottomOf="@+id/md12" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="10dp"
        android:text="@string/md"
        app:layout_constraintBottom_toTopOf="@+id/md11"
        app:layout_constraintStart_toStartOf="@+id/md11" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="10dp"
        android:text="@string/md2"
        app:layout_constraintBottom_toTopOf="@+id/md21"
        app:layout_constraintStart_toStartOf="@+id/md21" />

